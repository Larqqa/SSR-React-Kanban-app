{
  "name": "kanban-app",
  "version": "0.0.1",
  "description": "kanban style Express and SSR React app",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production npm run build && node build/server.js",
    "dev": "cross-env NODE_ENV=development npm-run-all devBuild:* && npm-run-all -p watch:*",
    "build": "rm -rf build && npm-run-all build:*",
    "build:server": "webpack --config=webpack.server.js --mode=production",
    "build:cli": "webpack --config=webpack.client.js --mode=production",
    "devBuild:server": "webpack --config=webpack.server.js --mode=development",
    "devBuild:cli": "webpack --config=webpack.client.js --mode=development",
    "watch:server": "webpack --watch --config=webpack.server.js --mode=development",
    "watch:nodemon": "nodemon ./devBuild/server.js",
    "watch:web": "webpack-dev-server --open --config=webpack.client.js --mode=development"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.8",
    "mongoose-unique-validator": "^2.0.3",
    "normalize.css": "^8.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-loader": "^0.5.5",
    "isomorphic-style-loader": "^5.1.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  }
}
